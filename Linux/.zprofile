export CC=colorgcc
export EDITOR=vim

export LD_LIBRARY_PATH=.:/usr/local/lib:$LD_LIBRARY_PATH

export XDG_USER_CONFIG=$HOME/.config
export XDG_USER_DATA=$HOME/.config
