export XCMENUCTRL_DMENU='dmenu -i -l 15'

export PATH=./node_modules/.bin:$HOME/.gem/ruby/2.1.0/bin:$HOME/bin:$PATH
export NODE_PATH=/usr/lib/node_modules:/usr/lib:.
