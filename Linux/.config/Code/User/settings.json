    }
  ],
  "vim.operatorPendingModeKeyBindingsNonRecursive": [
    {
      "before": ["("],
      "after": ["s", ")"]
    },
    {
      "before": [")"],
      "after": ["s", ")"]
    },
    {
      "before": ["["],
      "after": ["s", "]"]
    },
    {
      "before": ["]"],
      "after": ["s", "]"]
    },
    {
      "before": ["{"],
